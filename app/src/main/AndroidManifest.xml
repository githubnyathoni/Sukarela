<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sukarela">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_s"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_s"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CariRelawan4Activity"></activity>
        <activity android:name=".CariRelawan3Activity" />
        <activity android:name=".CariRelawan2Activity" />
        <activity android:name=".CariRelawanActivity" />
        <activity android:name=".Profile" />
        <activity android:name=".Login" />
        <activity android:name=".Register">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Login" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.dashboard.DetailsActivity" />
    </application>

</manifest>